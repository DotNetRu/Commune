{
    "Serilog": {
        "Using":  [ "Serilog.Sinks.BrowserConsole" ],
        "MinimumLevel": "Verbose",
        "WriteTo": [
            { "Name": "BrowserConsole" }
        ],
        "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
        "Properties": {
            "Application": "Sample"
        }
    },
    "AuditSettings":{
        "RepositoryName":"DEMO",
        "OriginalOwner":"SelectFromGroup-By"
    }
}
